{
  "name": "win-a-beer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-client && npm run build-server",
    "build-client": "ng build -prod -aot",
    "build-server": "webpack --config server/webpack.config.js",
    "docker-build": "docker build -t vfeskov/win-a-beer .",
    "docker-push": "docker push vfeskov/win-a-beer",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "node server/bundle.js",
    "test": "ng test",
    "watch-server": "concurrently \"webpack --config server/webpack.config.js --watch\" \"nodemon --watch server/bundle.js server/bundle.js\"",
    "watch-client": "ng serve --proxy-config proxy.conf.json -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.2",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.5",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "aws-sdk": "^2.50.0",
    "bcrypt": "^1.0.2",
    "boom": "^4.3.1",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "hapi": "^16.1.1",
    "hapi-auth-jwt2": "^7.2.4",
    "inert": "^4.2.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/bcrypt": "^1.0.0",
    "@types/boom": "^4.3.1",
    "@types/dotenv": "^4.0.0",
    "@types/hapi": "^16.0.6",
    "@types/jasmine": "2.5.38",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/node": "^6.0.73",
    "awesome-typescript-loader": "^3.1.3",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-loader": "^0.5.4",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "webpack-node-externals": "^1.6.0"
  }
}
